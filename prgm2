#include <stdio.h>
int main()
{
    int a,b,step,big;
    printf("enter num1:");
    scanf("%d",&a);
    printf("enter num2:");
    scanf("%d",&b);
    big=a>b?a:b;
    step=big;
    while(1)
    {
        if(big%a==0 && big%b==0)
        {
         printf("the lcm is %d",big);
         break;
        }
    else
       big=big+step;
    }
}
